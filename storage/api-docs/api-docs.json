        "/rest/v1/detalles": {
            "post": {
                "tags": [
                    "detalles"
                ],
                "summary": "Guardar un detalle de un pedido",
                "description": "Se almacena un nuevo detalle de un pedido",
                "operationId": "193a7cf77104a51ea157d97f7ebe050b",
                "requestBody": {
                    "description": "El detalle que se va a crear",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "id",
                                    "precio_total",
                                    "cantidad",
                                    "id_pedido",
                                    "id_producto"
                                ],
                                "properties": {
                                    "id": {
                                        "type": "integer",
                                        "example": "1"
                                    },
                                    "precio_total": {
                                        "type": "integer",
                                        "example": "200000"
                                    },
                                    "cantidad": {
                                        "type": "integer",
                                        "example": "3"
                                    },
                                    "id_pedido": {
                                        "type": "integer",
                                        "example": "2"
                                    },
                                    "id_producto": {
                                        "type": "integer",
                                        "example": "1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Detalle creado correctamente"
                    },
                    "400": {
                        "description": "Solicitud incorrecta"
                    },
                    "401": {
                        "description": "No autenticado"
                    },
                    "403": {
                        "description": "Prohibido"
                    },
                    "405": {
                        "description": "Entrada invalida"
                    }
                }
            }
        },
        "/rest/v1/pedidos": {
            "post": {
                "tags": [
                    "pedidos"
                ],
                "operationId": "66ba1aa467332b07c04505e382271815",
                "requestBody": {
                    "description": "El pedido que se va a crear",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "id",
                                    "fecha_pedido",
                                    "precio",
                                    "id_cliente"
                                ],
                                "properties": {
                                    "id": {
                                        "type": "integer",
                                        "example": "1"
                                    },
                                    "fecha_pedido": {
                                        "type": "string",
                                        "example": "2023-05-02"
                                    },
                                    "precio": {
                                        "type": "integer",
                                        "example": "203000"
                                    },
                                    "id_cliente": {
                                        "type": "integer",
                                        "example": "2"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Pedido creado correctamente"
                    },
                    "400": {
                        "description": "Solicitud incorrecta"
                    },
                    "401": {
                        "description": "No autenticado"
                    },
                    "403": {
                        "description": "Prohibido"
                    },
                    "405": {
                        "description": "Entrada invalida"
                    }
                }
            }
        },
